{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "containergroupname": {
            "value": "qdrantonazure"
        },
        "containername": {
            "value": "azqdrantcontainer"
        },
        "location": {
            "value": "westus"
        },
        "imageName": {
            "value": "mindmate.azurecr.io/qdrant"
        },
        "registryusername": {
            "value": "mindmate"
        },
        "registrypassword": {
            "value": "password"
        },
        "volumeName": {
            "value": "azqdrantvolume"
        },
        "numberCpuCores": {
            "value": 4
        },
        "memory": {
            "value": 16
        },
        "ipAddressType": {
            "value": "Public"
        },
        "storageaccountname": {
            "value": "qdrantstorage"
        },
        "storageaccountkey": {
            "value": "mykey"
        },
        "storagesharename": {
            "value": "aciqdrantshare"
        },
        "dnsNameLabel": {
            "value": "qdrant-aci-dns"
        }
    }
}
